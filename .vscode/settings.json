{
    "debug.javascript.terminalOptions": {
      "enableTurboSourcemaps": true,
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "node_modules/kubernetes-fluent-client/**",
        "node_modules/pepr/**"
      ]
    },
    "yaml.schemas": {
      "https://raw.githubusercontent.com/defenseunicorns/uds-cli/v0.9.0/uds.schema.json": [
        "**/uds-bundle.yaml"
      ],
  
      "https://raw.githubusercontent.com/defenseunicorns/uds-cli/v0.9.0/tasks.schema.json": [
        "tasks.yaml",
        "tasks/**/*.yaml",
        "src/**/validate.yaml"
      ],
      "https://raw.githubusercontent.com/defenseunicorns/uds-cli/v0.9.0/zarf.schema.json": [
        "**zarf.yaml"
      ]
    },
    "cSpell.words": [
      "alertmanager",
      "Authservice",
      "automount",
      "controlplane",
      "crds",
      "distros",
      "ironbank",
      "Kiali",
      "Kyverno",
      "MITM",
      "neuvector",
      "opensource",
      "promtail",
      "Quickstart",
      "seccomp",
      "Sysctls",
      "Velero"
    ]
  }
  